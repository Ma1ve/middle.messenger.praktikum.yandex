{
  "compilerOptions": {
     "outDir": "dist",
    "target": "es2016",
    "declaration": false,
    "module": "commonjs",

    "sourceMap": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true ,
    "noImplicitAny": true ,
    "strictNullChecks": true ,
    "noUnusedLocals": true ,
    "noUnusedParameters": true ,
    "skipLibCheck": true,
     "lib": [
      "ESNext",
      "DOM"
    ],
  }
}
